{
  "name": "gritos",
  "version": "1.0.0",
  "description": "Gritos.com platform",
  "main": "js/app/app.js",
  "scripts": {
    "lint": "eslint .",
    "cleanup": "rm -rf dist",
    "build": "npm run cleanup && NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js",
    "start": "npm run cleanup && NODE_ENV=development node ./webpack/webpack.dev.server.js",
    "deploy": "s3-deploy './dist/**' --cwd './dist/' --region $AWS_REGION --bucket $BUCKET_NAME/$CIRCLE_BUILD_NUM --gzip",
    "tag": "ci/tag.sh",
    "bump-version": "ci/bump-version.sh $OPS_FRONTEND_KEY $CIRCLE_BUILD_NUM",
    "now-build": "npm run build && ns ./dist -n frontend-project-template --cmd \"list ./content -s\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/nachocs/gritos.git"
  },
  "author": "Import.io",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/nachocs/gritos/issues"
  },
  "homepage": "https://github.com/nachocs/gritos#readme",
  "dependencies": {
    "autolinker": "^1.2.1",
    "backbone": "^1.3.3",
    "fb": "^1.1.1",
    "font-awesome": "^4.5.0",
    "jquery": "^3.1.1",
    "lodash": "^4.16.6",
    "moment": "^2.16.0",
    "requirejs": "^2.3.2",
    "text": "github:requirejs/text"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.1",
    "babel-core": "latest",
    "babel-eslint": "latest",
    "babel-loader": "latest",
    "babel-preset-es2015": "latest",
    "babel-preset-stage-0": "latest",
    "babel-register": "latest",
    "babel-runtime": "",
    "babelify": "latest",
    "chai": "^3.4.1",
    "chalk": "^1.1.1",
    "classnames": "^2.2.3",
    "css-loader": "^0.23.1",
    "core-decorators": "",
    "debug": "",
    "eslint": "latest",
    "eslint-plugin-babel": "latest",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.9.0",
    "less": "^2.6.0",
    "less-loader": "^2.2.2",
    "mocha": "^2.4.5",
    "postcss": "^5.2.5",
    "postcss-loader": "^1.0.0",
    "request": "",
    "s3-deploy": "^0.6.1",
    "sinon": "^1.17.2",
    "style-loader": "^0.13.0",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dashboard": "^0.2.0",
    "webpack-dev-server": "^1.14.1"
  }
}
