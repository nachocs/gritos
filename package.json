{
  "name": "gritos",
  "version": "1.8.4",
  "description": "Gritos.com",
  "main": "js/app/app.js",
  "scripts": {
    "lint": "eslint .",
    "cleanup": "rm -rf dist",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack/webpack.prod.config.js",
    "start": "npm run cleanup && cross-env NODE_ENV=development webpack-dev-server --progress --color --config ./webpack/webpack.dev.server.js",
    "deploy": "s3-deploy './dist/**' --cwd './dist/' --region $AWS_REGION --bucket $BUCKET_NAME/$CIRCLE_BUILD_NUM --gzip",
    "tag": "ci/tag.sh",
    "bump-version": "ci/bump-version.sh $OPS_FRONTEND_KEY $CIRCLE_BUILD_NUM",
    "now-build": "npm run build && ns ./dist -n gritos --cmd \"list ./content -s\"",
    "serve": "cross-env NODE_ENV=production ENDPOINTS_ROOT_DOMAIN=gritos.com node ./webpack/webpack.prod.server.js"
  },
  "repository": {
    "type": "git",
    "url": "git+git@github.com:nachocs/gritos.git"
  },
